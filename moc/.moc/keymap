go_to_fast_dir7       = &
go_to_fast_dir8       = *
go_to_fast_dir9       = (
go_to_fast_dir10      = )
go_to_playing_file    = .
go_up                 = -

# Playlist specific keys:
add_file              = a
add_directory         = A
plist_add_stream      = 
delete_from_playlist  = d
